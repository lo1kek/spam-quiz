{
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "functions": {
    "app.py": {
      "includeFiles": ["static/**/*", "templates/**/*", "config.json"]
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
